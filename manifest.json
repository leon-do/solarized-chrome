{
  "manifest_version": 3,
  "name": "Wikipedia Solarized Dark Theme",
  "version": "1.3",
  "icons": {
    "16": "icons/icon16.png",
    "18": "icons/icon18.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.wikipedia.org/*"],
      "css": ["css/root.css", "css/wikipedia.css"]
    }
  ]
}
